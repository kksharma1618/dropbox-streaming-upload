{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "eofline": false,
        "max-line-length": [true, 160],
        "member-access": false,
        "ordered-imports": [false],
        "object-literal-sort-keys": false,
        "no-var-requires": false,
        "semicolon": [
            "never"
        ],
        "quotemark": [
            "single"
        ],
        "trailing-comma": [
            false
		],
		"interface-name": [
            false
		],
        "no-empty": false,
        "one-variable-per-declaration": [
            false
        ]
    },
    "rulesDirectory": []
}